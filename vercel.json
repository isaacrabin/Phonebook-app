{
  "version": 2,
  "builds": [
    {
      "src": "dist/browser/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/browser/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
